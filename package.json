{
  "name": "chat-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./server.js",
    "watch": "nodemon ./server.js --ignore public/",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run watch\" \"npm run client\"",
    "build": "npm install && npm install --prefix client && npm run load-sample-data",
    "load-sample-data": "node ./sample-data/load.js",
    "delete-sample-data": "node ./sample-data/load.js --delete"
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.19.0",
    "connect-flash": "0.1.1",
    "connect-mongo": "1.3.2",
    "cookie-parser": "1.4.3",
    "dompurify": "0.8.5",
    "dotenv": "4.0.0",
    "es6-promisify": "^6.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "5.2.0",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.7.7",
    "mongoose-mongodb-errors": "0.0.2",
    "passport": "0.3.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "4.0.0",
    "socket.io": "^2.1.0",
    "uniqid": "^4.1.1",
    "validator": "7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.24.0",
    "concurrently": "3.4.0",
    "css-loader": "0.27.3",
    "eslint": "^4.19.1",
    "node-sass": "4.5.0",
    "nodemon": "^1.19.4"
  }
}
